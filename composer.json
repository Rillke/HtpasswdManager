{
  "name": "iresults/htpasswd_manager",
  "description": "htpasswd file manager",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/lumen-framework": "5.2.*",
    "vlucas/phpdotenv": "~2.2",
    "twbs/bootstrap": "v4.0.0-alpha.2",
    "fortawesome/font-awesome": "4.1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0",
    "cundd/test-flight": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  }
}
